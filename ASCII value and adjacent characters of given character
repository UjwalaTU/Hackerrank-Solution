#include <stdio.h>
int main() {
    char ch;
    scanf("%c",&ch);
    if(ch>=65 && ch<90)
    {
        printf("%d\n",ch);
        printf("%c %c",ch-1,ch+1);
    }
    else if(ch>=97 && ch<122)
    {
        printf("%d\n",ch);
        printf("%c %c",ch-1,ch+1);
    }
    else if(ch==90)
    {
        printf("%d\n",ch);
        printf("%c %c",ch-1,65);
    }
    else if(ch==122)
    {
        printf("%d\n",ch);
        printf("%c %c",ch-1,97);
    }
    return 0;
}
